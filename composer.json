{
  "name": "ichhabrecht/git-wrapper",
  "description": "A PHP (read-only) wrapper for the Git command line utility",
  "keywords": [
    "Git",
    "Wrapper",
    "PHP",
    "Utility"
  ],
  "homepage": "https://github.com/IchHabRecht/git-wrapper",
  "authors": [
    {
      "name": "Nicole Cordes",
      "role": "Developer",
      "homepage": "https://twitter.com/IchHabRecht"
    }
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "symfony/process": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "symfony/filesystem": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "IchHabRecht\\GitWrapper\\": "src/GitWrapper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IchHabRecht\\GitWrapper\\Test\\": "tests/GitWrapper/"
    }
  }
}
